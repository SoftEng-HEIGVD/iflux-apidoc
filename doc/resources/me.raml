/organizations:
  is: [ auth ]
  get:
    securedBy: [ jwt: { type: 'owner' }, test ]
    description: Retrive the list of all organization where the is a member.
    responses:
      200:
        description: List of organizations or empty list `[]`.
        body:
          schema: organization-getAll
          example: !include ../examples/organizations/response-array-get.json
