{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Action target instance",
  "type": "object",
  "properties": {
    "id": {
      "description": "Unique identifier given by the server",
      "type": "integer"
    },
    "actionTargetInstanceId": {
      "description": "Unique id generated by the server.",
      "type": "string"
    },
    "name": {
      "description": "A name to identify the action target instance.",
      "type": "string"
    },
    "actionTargetTemplateId": {
      "description": "The action target template instantiated.",
      "type": "integer"
    },
    "configuration": {
      "description": "A valid JSON object.",
      "type": "json"
    }
  },
  "required": [ "id", "actionTargetInstanceId", "name", "actionTargetTemplateId", "configuration" ]
}